{
  "hosting": {
    "public": "public",

    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],

    "rewrites": [
      {
        "source": "/netdiag",
        "destination": "/media/netdiag.png"
      },
      {
        "source": "/resume",
        "destination": "/media/resume.pdf"
      }
    ],



    "cleanUrls": true,

    "trailingSlash": false
  }
}
